<h1>Listing cars</h1>

<table>
  <tr>
    <th>Mark</th>
    <th>Model</th>
    <th>Thumbnailurl</th>
    <th>Picture</th>
    <th>Comment</th>
    <th>Cc</th>
    <th>Kmpassed</th>
    <th>Automatic</th>
    <th>Centerlock</th>
    <th>Powersteering</th>
    <th>Abs</th>
    <th>Electricwindow</th>
    <th>Airbag</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cars.each do |car| %>
  <tr>
    <td><%= car.mark %></td>
    <td><%= car.model %></td>
    <td><%= car.thumbnailurl %></td>
    <td><%= car.picture %></td>
    <td><%= car.comment %></td>
    <td><%= car.cc %></td>
    <td><%= car.kmpassed %></td>
    <td><%= car.automatic %></td>
    <td><%= car.centerlock %></td>
    <td><%= car.powersteering %></td>
    <td><%= car.abs %></td>
    <td><%= car.electricwindow %></td>
    <td><%= car.airbag %></td>
    <td><%= link_to 'Show', car %></td>
    <td><%= link_to 'Edit', edit_car_path(car) %></td>
    <td><%= link_to 'Destroy', car, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Car', new_car_path %>
